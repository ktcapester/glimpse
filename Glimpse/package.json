{
  "name": "card-pricer",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "sync:browser": "aws s3 sync dist/Glimpse/browser/ s3://%GLIMPSE_S3_BUCKET%/ --delete",
    "sync:prerendered": "aws s3 cp dist/Glimpse/prerendered-routes.json s3://%GLIMPSE_S3_BUCKET%/",
    "sync:licenses": "aws s3 cp dist/Glimpse/3rdpartylicenses.txt s3://%GLIMPSE_S3_BUCKET%/",
    "sync:root": "npm run sync:prerendered && npm run sync:licenses",
    "sync:aasa": "aws s3 cp public/apple-app-site-association s3://%GLIMPSE_S3_BUCKET%/.well-known/ --content-type application/json",
    "sync": "npm run sync:browser && npm run sync:root && npm run sync:aasa",
    "invalidate": "aws cloudfront create-invalidation --distribution-id %GLIMPSE_CLOUDFRONT_DIST_ID% --paths /* --no-cli-pager",
    "deploy": "npm run build && npm run sync && npm run invalidate",
    "docs_update": "jsdoc -c jsdoc.json --verbose"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/router": "^20.0.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "mongodb": "^6.12.0",
    "mongoose": "^8.9.5",
    "nodemailer": "^6.10.0",
    "rxjs": "~7.8.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~5.7.0",
    "jest": "^30.0.0",
    "jsdoc": "^4.0.4",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.1.7",
    "supertest": "^7.1.1",
    "typescript": "~5.8.3"
  }
}
